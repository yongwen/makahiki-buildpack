#!/usr/bin/env bash

# This script run any makahiki init setup
#
# This script is invoked by [`bin/steps/hooks/post_compile`](/).

# Syntax sugar.
source $BIN_DIR/utils

echo "-----> set up makahiki instance"

fixture = "default_all.json"
python makahiki/manage.py syncdb --noinput --migrate --verbosity 0

python makahiki/manage.py setup_test_data rounds 1
python makahiki/manage.py loaddata -v 0 makahiki/fixtures/default_all.json
python makahiki/manage.py setup_test_data all 2

